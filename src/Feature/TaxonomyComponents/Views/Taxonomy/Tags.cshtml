@using Sitecore.Mvc
@using Sitecore.Data.Items
@using Foundation.TaxonomyInSitecore.Configuration;

@{
    Sitecore.Data.Fields.MultilistField multiselectField = Sitecore.Context.Item.Fields[ConfigSettings.TaxonomyField];
    Sitecore.Data.Items.Item[] tags = multiselectField.GetItems();
}
@if (tags.Any())
{
    foreach (Item tag in tags)
    {
		Html.RenderPartial("~/Views/Taxonomy/Tag.cshtml", tag);
    }
}